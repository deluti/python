x=0
y=0
clr=0
clr1=0
clr2=0
f=20
x1=0
f1=0
def setup():
    size(1800,1000)
def draw():
    fill(clr,clr1,clr2)
    translate(900,500)
    rotate(radians(y))
    circle(x,0,f)
    rect(x,0,f,f)
    rect(x1,0,f1,f1)
    global x,y,clr,clr1,clr2,f,f1,x1
    x+=0.1
    y+=1
    clr+=0.01
    clr1=12.5
    clr2=78
    f+=0.04
    f1-=0.04
    x1+=0.1
