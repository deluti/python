import pyautogui
import time


pyautogui.moveTo(500,520)

pyautogui.click(button = 'right')

pyautogui.moveTo(530,450)
time.sleep(1)
pyautogui.moveTo(870,560)
pyautogui.click()
pyautogui.moveTo(500,510)
pyautogui.write('dentist', interval=0.1)
pyautogui.doubleClick()
time.sleep(3)
pyautogui.write('dentist at 8:00', interval=0.1)

pyautogui.hotkey('ctrl','s')
pyautogui.hotkey('alt','F4')
